#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin >> t;
    while (t--) {
        int n;
        cin >> n;
        int x;
        cin >> x;
        for (int i = 1; i < n; ++i) {
            int y;
            cin >> y;
            x &= y;
        }
        cout << x << "\n";
    }
}
